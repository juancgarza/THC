<%= link_to 'Back to dealerships', dealerships_path %>

<h1>
  <%= @dealership.name %>
</h1>

<table class="styled-table">
  <thead>
    <tr>
      <th>Model</th>
      <th>Manufacturer</th>
      <th>Color</th>
      <th>Price</th>
      <th>Quantity</th>
    </tr>
  </thead>
  <tbody>
    <% @vehicles.each do |vehicle| %>
    <tr>
        <td><%= vehicle.model %></td>
        <td><%= vehicle.manufacturer %></td>
        <td><%= vehicle.color %></td>
        <td><%= vehicle.price %></td>
        <td><%= vehicle.item.quantity %></td>
    </tr>
    <% end %>
  </tbody>
</table>